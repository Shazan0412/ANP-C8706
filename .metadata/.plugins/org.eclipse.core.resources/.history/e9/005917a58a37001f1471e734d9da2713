package jul1;

public class Sum {
  
	       static int sum(int num1, int num2) {
	           return num1 + num2;
	       }
				
	       public static void main(String[] args) {
	    	   sum s = new sum();
	           int result = s.sum(5,
	        		   7);
	           System.out.println("The sum of 5 and 7 is: " + result);
	       }
	   }
